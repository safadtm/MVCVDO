@model IEnumerable<MVCVDO.Models.Users>

<p>
    @Html.ActionLink("Home", "AdminHome")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>

        <td>
            @Html.ActionLink("Add","EmailAndPasswordSend", new { id = item.id }) | 
            @Html.ActionLink("Delete", "DeleteEmail", new { id = item.id })
        </td>
    </tr>
}

</table>
